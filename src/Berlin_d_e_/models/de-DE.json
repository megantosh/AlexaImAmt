    "prompts": [
      {
        "id": "Elicit.Slot.731925008572.784370516716",
        "variations": [
          {
            "type": "PlainText",
            "value": "ich verstehe du suchst nach einer Approbation für einen akademischen Beruf im Gesundheitswesen. Welcher soll der sein?"
          }
        ]
      },
      {
        "id": "Elicit.Slot.194404890129.1162370056458",
        "variations": [
          {
            "type": "PlainText",
            "value": "ich gehe davon aus, dass deine Examen in Berlin waren?"
          },
          {
            "type": "PlainText",
            "value": "und deine prüfung dafür ist schon in Berlin bestanden?"
          },
          {
            "type": "PlainText",
            "value": "Hast du dafür deine Prüfung in Berlin abgelegt?"
          }
        ]
      },
      {
        "id": "Elicit.Slot.705370932563.596480606902",
        "variations": [
          {
            "type": "PlainText",
            "value": "Du weißt wahrscheinlich schon, dass du dafür in Berlin angemeldet sein musst. Bist du in Berlin schon angemeldet?"
          },
          {
            "type": "PlainText",
            "value": "Um diese Angelegenheit zu bearbeiten, musst du in Berlin angemeldet sein. Bist du in Berlin angemeldet?"
          }
        ]
      },
      {
        "id": "Elicit.Slot.705370932563.1089481707959",
        "variations": [
          {
            "type": "PlainText",
            "value": "Super. ich brauche etwas mehr information über dich, wenn ich darf. Ich gehe davon aus, du verrätst mir nur die Staatsbürgerschaft, mit der du den Antrag stellen willst.  Also.. Was ist deine Staatsbürgerschaft?"
          },
          {
            "type": "PlainText",
            "value": "OK, mit welcher Staatsbürgerschaft willst du verfahren?"
          }
        ]
      },
      {
        "id": "Elicit.Slot.705370932563.1198061985282",
        "variations": [
          {
            "type": "PlainText",
            "value": "und was ist der Aufenthaltszweck?"
          }
        ]
      },
      {
        "id": "Elicit.Slot.705370932563.860066613759",
        "variations": [
          {
            "type": "PlainText",
            "value": "soll ich dir was über die Voraussetzungen dazu erzählen?"
          },
          {
            "type": "PlainText",
            "value": "magst du dich über die Voraussetztungen informieren?"
          }
        ]
      },
      {
        "id": "Elicit.Slot.705370932563.650345028907",
        "variations": [
          {
            "type": "PlainText",
            "value": "ist das eine Verlängerung eines bestehenden Aufenthaltstitels?"
          }
        ]
      },
      {
        "id": "Elicit.Slot.170695308429.1485160893156",
